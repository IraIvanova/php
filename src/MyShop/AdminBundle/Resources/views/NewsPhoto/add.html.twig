{% extends "@MyShopAdmin/layout.html.twig" %}

{% block body %}
    <h3>Добавление фото к товару {{news.title }}</h3>
    <form enctype="multipart/form-data" method="post" action="{{ path("my_shop_admin.news_photo_add", {idNews:news.getId()}) }}">
        {{ form_widget(form) }}
        <input type="submit" value="Добавить" />
    </form>
{% endblock %}