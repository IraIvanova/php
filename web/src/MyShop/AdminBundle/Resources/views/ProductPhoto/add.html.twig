{% extends "@MyShopAdmin/layout.html.twig" %}

{% block body %}
    <h3>Добавление фото к товару {{ product.model }}</h3>
    <form enctype="multipart/form-data" method="post" action="{{ path("my_shop_admin.product_photo_add", {idProduct:product.getId()}) }}">
        {{ form_widget(form) }}
        <input type="submit" value="Добавить" />
    </form>
{% endblock %}